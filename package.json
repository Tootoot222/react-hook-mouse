{
  "name": "react-typescript-hook-mouse",
  "version": "1.0.1",
  "description": "React hook for mouse position, movement and buttion states, now with Typescript support",
  "keywords": [
    "react",
    "hook",
    "mouse",
    "typescript"
  ],
  "repository": "git@github.com:Tootoot222/react-typescript-hook-mouse.git",
  "license": "LGPL-3.0",
  "author": "Bence A. Toth <tothab@gmail.com>, Tyler Brown <tybrownf@gmail.com>",
  "main": "dist/index.js",
  "scripts": {
    "eslint::check": "eslint --ext .js,.ts src",
    "eslint::fix": "eslint --fix --ext .js,.ts src",
    "typescript::check": "tsc",
    "build": "rm -rf dist && tsc && eslint --ext .js,.ts src && babel src --extensions .ts,.js --out-dir dist",
    "build:watch": "babel src --out-dir dist --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "babel-core": "^7.0.0-bridge",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "react": "^16.12.0",
    "typescript": "^3.7.3"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  }
}
